package kr.or.ddit.dam.vo;

public class GiftVO {
	private int gift_no;
	private String gift_name;
	private String gift_image;
	private int gift_amt;
	
	public int getGift_no() {
		return gift_no;
	}
	public void setGift_no(int gift_no) {
		this.gift_no = gift_no;
	}
	public String getGift_name() {
		return gift_name;
	}
	public void setGift_name(String gift_name) {
		this.gift_name = gift_name;
	}
	public String getGift_image() {
		return gift_image;
	}
	public void setGift_image(String gift_image) {
		this.gift_image = gift_image;
	}
	public int getGift_amt() {
		return gift_amt;
	}
	public void setGift_amt(int gift_amt) {
		this.gift_amt = gift_amt;
	}
	
	
}
